<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Company>MultiNet Interactive AB</Company>
    <Authors>Chris Gårdenberg</Authors>
    <Description>MultiNet L10n
Översättningspaket</Description>
    <PackageTags>L10n translation</PackageTags>
    <RepositoryUrl>http://phoenix.net.multinet.se/general/mn-l10n</RepositoryUrl>
    <PackageProjectUrl>http://phoenix.net.multinet.se/general/mn-l10n</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <Copyright>© 20XX MultiNet Interactive AB</Copyright>
    <Version>1.0.54</Version>
	<AutoIncrementPackageRevision>True</AutoIncrementPackageRevision>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Javascript\L10n.js" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Javascript\L10n.js" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CommonMark.NET" Version="0.15.1" />
    <PackageReference Include="Jil" Version="2.15.0" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="StackExchange.Precompilation.Build" Version="4.0.0" />
    <PackageReference Include="System.Reflection.Metadata" Version="1.2.0" />
    <PackageReference Include="WebGrease" Version="1.6.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>